# Monitoring Configuration
# Copy this file to .env.local and add your actual keys

# Sentry Error Tracking
# Get your DSN from https://sentry.io/
VITE_SENTRY_DSN=

# PostHog Analytics
# Get your key from https://posthog.com/
VITE_POSTHOG_KEY=
VITE_POSTHOG_HOST=https://app.posthog.com

# App Version (optional)
VITE_APP_VERSION=1.0.0

# Build Target
# clean: No ads, for Vercel/Pages deployment
# crazy: With ads, for portal deployment
VITE_BUILD_TARGET=clean

# Ad Integration
VITE_ENABLE_ADS=false
VITE_ADSENSE_CLIENT_ID=ca-pub-XXXXXXXXXXXXXXXX
