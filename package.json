{
  "name": "tictactoe-rolling",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:clean": "cross-env VITE_BUILD_TARGET=clean vite build",
